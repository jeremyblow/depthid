{
  "controller": {
    "device_name": "/dev/cu.usbmodem211",
    "baud_rate": 115200,
    "motors": [
      ["x", 1.00, 0],
      ["y", 0.50, 0],
      ["z", 1.00, 0]
    ]
  },
  "camera": {
    "interface": "spinnaker",
    "camera_index": 0,
    "height": 488,
    "width": 648,
    "capture_format": "Mono8"
  },
  "job": {
    "name": "interactive_session",
    "path": "data/",
    "save_formats": ["raw", "tiff"],
    "display": true,
    "mode": "interactive",
    "wait_after": 0.1
  }
}