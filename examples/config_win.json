{
  "controller": {
    "interface": "grbl",
    "device_name": "COM4",
    "baud_rate": 115200,
    "motors": [
      ["x", 1.00],
      ["y", 1.00],
      ["z", 1.00]
    ]
  },
  "camera": {
    "interface": "spinnaker",
    "camera_index": 0,
    "height": 1200,
    "width": 1920,
    "pixel_format": "Mono 16",
    "display_format": "Mono 16",
    "save_formats": ["raw", "tiff"],
    "exposure_us": 54725.17013549805,
    "gain_db": 15
  },
  "job": {
    "name": "interactive_session",
    "path": "data/",
    "mode": "interactive",
    "pipeline": [
      {"m": "spinnaker", "f": "capture", "i": "camera", "kw": {"wait_before":  0.0, "wait_after": 0.0}},
      {"m": "spinnaker", "f": "transform_ndarray", "i": 0, "kw": {}},
      {"m": "opencv", "f": "gray_to_rgb", "i": 1, "kw": {}},
      {"m": "mpl", "f": "plot_histogram_grey", "i":  1, "kw": {"bins": 65536}}
    ]
  }
}